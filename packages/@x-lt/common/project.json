{
  "name": "common",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/@x-lt/common/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "packages/@x-lt/common/**/*.ts"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/packages/@x-lt/common"
      ],
      "options": {
        "jestConfig": "packages/@x-lt/common/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
