{
  "name": "@ng-atomic/dashboard",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "packages/@ng-atomic/dashboard/src",
  "prefix": "app",
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/@ng-atomic/dashboard",
        "index": "packages/@ng-atomic/dashboard/src/index.html",
        "main": "packages/@ng-atomic/dashboard/src/main.ts",
        "polyfills": ["zone.js"],
        "tsConfig": "packages/@ng-atomic/dashboard/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          "packages/@ng-atomic/dashboard/src/favicon.ico",
          "packages/@ng-atomic/dashboard/src/assets"
        ],
        "styles": [
          "node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
          "packages/@ng-atomic/dashboard/src/styles.scss"
        ],
        "stylePreprocessorOptions": {
          "includePaths": [
            "node_modules/host-css-variable",
            "packages/@ng-atomic/components/src/styles"
          ]
        },
        "scripts": []
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "options": {
        "headers": {
          "Cross-Origin-Opener-Policy": "same-origin",
          "Cross-Origin-Embedder-Policy": "require-corp"
        }
      },
      "configurations": {
        "production": {
          "browserTarget": "@ng-atomic/dashboard:build:production"
        },
        "development": {
          "browserTarget": "@ng-atomic/dashboard:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "@ng-atomic/dashboard:build"
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "packages/@ng-atomic/dashboard/**/*.ts",
          "packages/@ng-atomic/dashboard/**/*.html"
        ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/@ng-atomic/dashboard/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    }
  },
  "tags": []
}
