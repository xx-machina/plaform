{
  "extends": ["@schematics/angular"],
  "schematics": {
    "auto": {
      "description": "Generate auto-detected files by GPT-3",
      "factory": "./lib/schematics/auto/index#auto",
      "schema": "./lib/schematics/auto/schema.json",
      "aliases": ["a", "x"]
    },
    "file": {
      "description": "Generate a file by GPT-3",
      "factory": "./lib/schematics/file/index#file",
      "schema": "./lib/schematics/file/schema.json",
      "aliases": ["f"]
    },
    "directory": {
      "description": "Generate a directory by GPT-3",
      "factory": "./lib/schematics/directory/index#directory",
      "schema": "./lib/schematics/directory/schema.json",
      "aliases": ["dir", "d"]
    },
    "instruct": {
      "description": "Generate code by custom instructions to GPT-3",
      "factory": "./lib/schematics/instruct/index#instruct",
      "schema": "./lib/schematics/instruct/schema.json",
      "aliases": ["i"]
    },
    "ng-add": {
      "description": "Add GPT-3 to your project",
      "factory": "./lib/schematics/ng-add/index#ngAdd",
      "schema": "./lib/schematics/ng-add/schema.json"
    },
    "set-default-collection": {
      "description": "Set a default collection for the workspace",
      "factory": "./lib/schematics/set-default-collection/index#setDefaultCollection",
      "schema": "./lib/schematics/set-default-collection/schema.json",
      "private": true
    }
  }
}
